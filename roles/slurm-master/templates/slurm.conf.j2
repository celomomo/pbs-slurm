ClusterName=cluster
SlurmctldHost={{ SlurmctldHost }}

MailProg=/usr/bin/mail
PrologFlags=X11,contain
ProctrackType=proctrack/cgroup
SlurmctldPidFile={{ SLURM_VAR_DIR }}/run/slurmctld.pid
SlurmctldPort=6817
SlurmdPidFile={{ SLURM_VAR_DIR }}/run/slurmd.pid
SlurmdPort=6818
SlurmdSpoolDir={{ SLURM_VAR_DIR }}/spool/slurmd
SlurmUser={{ SLURM_USER }}
StateSaveLocation={{ SLURM_VAR_DIR }}/spool/slurmctld
SwitchType=switch/none
TaskPlugin=task/affinity,task/cgroup
SchedulerType=sched/backfill
AccountingStorageType=accounting_storage/slurmdbd
JobCompType=jobcomp/none
JobAcctGatherFrequency=30
JobAcctGatherType=jobacct_gather/cgroup
SlurmctldLogFile={{ SLURM_VAR_DIR }}/log/slurmctld.log
SlurmdLogFile={{ SLURM_VAR_DIR }}/log/slurmd.log
SrunPortRange=60001-63000
PreemptMode=requeue
PreemptType=preempt/qos

NodeName={{ ComputeNodes }} CPUs={{ CPUs }} Boards={{ Boards }}  SocketsPerBoard={{ SocketsPerBoard }} CoresPerSocket={{ CoresPerSocket }} ThreadsPerCore={{ ThreadsPerCore }} RealMemory={{ RealMemory }}
PartitionName={{ PartitionName }} Nodes={{ NODES }} Default=YES State=UP PreemptMode=requeue
